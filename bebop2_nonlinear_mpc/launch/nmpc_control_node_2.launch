<launch>
    <node   name='nmpc_control_node_2'
            pkg='bebop2_nonlinear_mpc'
            type='bebop_nmpc_node.py'
            output='screen'>

            <remap from="/mav_sim_odom" to="/bebop2/odom"/>
            <remap from="/bebop/pose" to="/vrpn_client_node/bebop2/pose"/>
            <remap from="/bebop/twist" to="/vrpn_client_node/bebop2/twist"/>
            <remap from="/bebop_pose_goal" to="/bebop2/pose_goal"/>
            <remap from="/mpc/trajectory_plan_vis" to="/bebop2/mpc/trajectory_plan_vis"/>
            <remap from="/bebop_auto/cmd_vel" to="/bebop2/auto_cmd_vel"/>


    </node>
</launch>
